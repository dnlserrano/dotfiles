[alias]
  co = checkout
  ci = commit -v
  cp = cherry-pick
  st = status
  br = branch
  fp = push --force-with-lease origin HEAD
  psh = push origin HEAD
  pl = pull
  df = diff
  dc = diff --cached
  sa = stash
  sp = stash pop
  ft = fetch
  ll = log --pretty=format:'%h %s'
[filter "lfs"]
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
